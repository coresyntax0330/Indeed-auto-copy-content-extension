{
  "manifest_version": 3,
  "name": "Indeed Content Copier",
  "version": "1.0",
  "description": "Copy insights content from Indeed pages",
  "permissions": ["clipboardWrite"],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
